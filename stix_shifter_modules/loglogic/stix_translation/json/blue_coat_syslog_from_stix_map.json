{
  "ipv4-addr": {
    "fields": {
      "value": ["ll_sourceIP", "ll_targetIP"]
    }
  },
  "ipv6-addr": {
    "fields": {
      "value": ["ll_sourceIP", "ll_targetIP"]
    }
  },
  "url": {
    "fields": {
      "value": ["ll_url"]
    }
  },
  "file": {
    "fields": {
      "hashes.SHA-256": ["sys_body"],
      "hashes.MD5": ["sys_body"],
      "hashes.SHA-1": ["sys_body"]
    }
  },
  "network-traffic": {
    "fields": {
      "dst_port": ["ll_targetPort"],
      "start": ["ll_eventStartTime"],
      "protocols[*]": ["ll_protocolName"],
      "src_ref.value": ["ll_sourceIP", "ll_sourceIP"],
      "dst_ref.value": ["ll_targetIP", "ll_targetIP"]
    }
  }
}
